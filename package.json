{
  "name": "gulp-redis-lua2js",
  "version": "3.0.0",
  "description": "Convert lua scripts to node modules",
  "main": "dist/index.cjs",
  "type": "module",
  "engines": {
    "node": ">=12.2"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "lint": "eslint --cache .",
    "build": "rollup -c",
    "test": "mocha",
    "cover": "c8 npm test",
    "prepare": "npm run build"
  },
  "files": [
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dotcore64/gulp-redis-lua2js.git"
  },
  "keywords": [
    "gulp",
    "gulpplugin",
    "redis",
    "lua"
  ],
  "author": "Julian Grinblat",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dotcore64/gulp-redis-lua2js/issues"
  },
  "homepage": "https://github.com/dotcore64/gulp-redis-lua2js#readme",
  "dependencies": {
    "plugin-error": "^1.0.1",
    "redis-lua2js": "^3.0.0",
    "through2": "^4.0.2",
    "vinyl-contents-tostring": "4.1.1"
  },
  "devDependencies": {
    "c8": "^7.11.2",
    "chai": "^4.3.6",
    "chai-as-promised": "^7.1.1",
    "dirname-filename-esm": "^1.1.1",
    "eslint": "^8.14.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-mocha": "^10.0.4",
    "eslint-plugin-node": "^11.1.0",
    "mocha": "^9.2.2",
    "module-from-string": "^3.1.4",
    "p-event": "^5.0.1",
    "rollup": "^2.70.2",
    "vinyl": "^2.2.1"
  }
}
