    "gulp-babel": "^6.1.2",
    "gulp-eslint": "^2.0.0",
    "gulp-mocha": "^2.2.0",
    "gulp-util": "^3.0.7",
    "isparta": "^4.0.0",
    "mocha": "^2.5.3",
    "require-from-string": "^1.2.0",
    "vinyl": "^1.1.1"
  }
